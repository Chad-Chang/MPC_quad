<mujoco model = "template">
	<compiler angle="radian"/>

	<option timestep="0.001" integrator="RK4" gravity = "0 0 -9.81">
      <flag energy="enable" contact="disable" />
    </option>

	<include file="basic_scene.xml"/>
	<worldbody>
        <light diffuse=".5 .5 .5" pos="0 0 3" dir="0 0 -1"/>
		<body pos="0 0 1.5">
			<inertial pos="0 0 0" euler="0 0 0" mass="2" diaginertia="0.003 0.005 0.005"/>
			<geom type="box" size=".3 .1 .1" rgba="0 .9 0 1"/>
						
			<body name="Link1" pos="0.6 0 0" euler="0 0 0">
            	<inertial pos="0.2 0 0" euler="0 0 0" mass="2.108" diaginertia="0.003 0.005 0.005"/>
            	<geom type="box" size=".3 .1 .1" rgba="0 0 .9 1"/>
            	<joint name="J1" type="hinge" pos="-0.3 0 0" axis="0 1 0" damping="1"/>
				
				<body name="Link2" pos="0.6 0 0" euler="0 0 0">
            		<inertial pos="0.2 0 0" euler="0 0 0" mass="2.108" diaginertia="0.003 0.005 0.005"/>
            		<geom type="box" size=".3 .1 .1" rgba="0.65 0 0.25 1"/>
            		<joint name="J2" type="hinge" pos="-0.3 0 0" axis="0 1 0" damping="1"/>
				</body>
			</body>
		</body>
	</worldbody>
</mujoco>
